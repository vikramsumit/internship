# Step 5: Create seasonal indicators
print('Creating seasonal indicators...')

def get_season(month):
    try:
        if pd.isna(month):
            return 'Unknown'
        month = int(month)
        if month in [12, 1, 2]:
            return 'Winter'
        elif month in [3, 4, 5]:
            return 'Spring'
        elif month in [6, 7, 8]:
            return 'Summer'
        elif month in [9, 10, 11]:
            return 'Fall'
        else:
            return 'Unknown'
    except:
        return 'Unknown'

features_clean['Season'] = features_clean['Month'].apply(get_season)
